<!--
 function validate(string) {
     if (!string) return false;
     var Chars = "0123456789 ";

     for (var i = 0; i < string.length; i++) {
        if (Chars.indexOf(string.charAt(i)) == -1)
           return false;
     }
     return true;
 }
 //-->

